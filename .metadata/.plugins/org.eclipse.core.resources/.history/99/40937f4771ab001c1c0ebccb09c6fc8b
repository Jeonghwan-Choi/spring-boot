<mapper namespace="BoardDAO">
    <!--  
    <select id="boardList" resultType="Hashmap">;
        SELECT
          email,name
        FROM Cart
    </select>
 -->
 
	 <select id="basicList" resultType="Hashmap">;
	        SELECT
	          id,date,name,customer,sqm,number,people
	        FROM basic_info
	 </select>
	 
	 <select id="basicCount" resultType="Integer">;
	        SELECT
	          count(*)'id'
	        FROM basic_info
	 </select>
    
     <select id="basicName" resultType="String">;
	        SELECT
	          name
	        FROM basic_info where id='3'
	 </select>
</mapper>
